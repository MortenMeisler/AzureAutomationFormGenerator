@model AzureRunbookFormViewModel

@{
    Layout = "_Layout";
}
<body>
    <table class="table">
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
    <div class="container col-md-5">
        <vc:azure-runbook-form resource-group="@Model.ResourceGroup" automation-account="@Model.AutomationAccount" runbook-name="@Model.RunbookName"></vc:azure-runbook-form>
    </div>
</body>






